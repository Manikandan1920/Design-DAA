public class PrintNumbersReverse {
    public static void printNumbers(int n) {
        if (n <= 0) {  
            return;
        }
        System.out.println(n);
        printNumbers(n - 1);  
    }

    public static void main(String[] args) {
        int N = 6;  
        printNumbers(N);  
    }
}

6
5
4
3
2
1
